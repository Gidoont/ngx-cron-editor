{
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "description": "A cron expression generator for Angular 10",
  "name": "@gidoont/ngx-cron-editor",
  "version": "0.0.8",
  "repository": {
    "type": "git",
    "url": "git://github.com/Gidoont/ngx-cron-editor"
  },
  "keywords": [
    "angular",
    "cron",
    "cron-expression",
    "angular-cli",
    "quartz"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Gidoont/ngx-cron-editor/issues"
  },
  "homepage": "https://github.com/Gidoont/ngx-cron-editor",
  "peerDependencies": {
    "@angular/core": "~8.2.14",
    "@angular/animations": "~8.2.14",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@angular/material": "^8.2.3",
    "rxjs": "~6.5.3",
    "zone.js": "~0.9.1"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "ngPackage": {
    "lib": {
      "entryFile": "public_api.ts",
      "styleIncludePaths": [
        "./src"
      ]
    },
    "dest": "../../dist/libs/ngx-cron-editor"
  }
}
